    "react-cookies": "^0.1.1",
    "react-dom": "^16.13.1",
    "react-helmet": "5.2.1",
    "react-loadable": "^5.5.0",
    "react-loading": "^2.0.3",
    "react-qr-reader": "^2.2.1",
