{
  "name": "screen-reader-adventures",
  "version": "1.2.0",
  "main": "index.js",
  "author": "George Gillams <open-source@georgegillams.co.uk>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/georgegillams/screen-reader-adventures.git"
  },
  "scripts": {
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf ./build",
    "link-helpers:clean": "rm -f $(pwd)/src/helpers $(pwd)/server/helpers",
    "dev": "redis-server & cross-env NODE_ENV=development SECRET_API_KEY=asdfghjkl STARTED_AT=$( date -u +%s ) BUILT_AT=$( date -u +%s ) nodemon --ignore 'server/server_content/*' --ignore 'src/*' --ignore 'build/*' --exec babel-node --inspect ./server/app.js src",
    "build:client": "next build",
    "build:server:copy-assets": "cp next.config.js ./build/ && cp -R ./public ./build/ && cp package.json ./build/ && cp package-lock.json ./build/",
    "build:shared": "cross-env babel -d ./build/shared ./shared -s --copy-files",
    "build:server": "cross-env babel -d ./build/server ./server -s --copy-files && npm run build:server:copy-assets",
    "build:replace-paths:linux": "find build -name \"*.js\" -exec sed -i 's/require(\"\\/.*\\/node_modules\\//require(\"/g' {} \\;",
    "build:replace-paths:mac": "find build -name \"*.js\" -exec sed -i '.bak' 's/require(\"\\/.*\\/node_modules\\//require(\"/g' {} \\;",
    "build:replace-paths": "if [[ \"$OSTYPE\" == \"darwin\"* ]]; then npm run build:replace-paths:mac; else npm run build:replace-paths:linux; fi;",
    "build": "BUILT_AT=$( date -u +%s ) npm run build:client && npm run build:shared && npm run build:server && npm run build:replace-paths",
    "build:test": "cross-env PORT=9001 PROJECT_UNDER_TEST=true npm run build",
    "start": "npm run link-helpers:clean && cross-env STARTED_AT=$( date -u +%s ) NODE_ENV=production node ./build/server/app.js",
    "start:test": "cross-env PORT=9001 PROJECT_UNDER_TEST=true npm run start",
    "start:aws": "npm run start",
    "test": "cross-env jest --coverage",
    "test:watch": "cross-env jest --watchAll",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --watchAll",
    "backstopjs:test": "npm run start:test & (sleep 15 && node ./scripts/backstopjs/backstop.js)",
    "backstopjs:test:allow-failure": "npm run start:test & (sleep 15 && node ./scripts/backstopjs/backstop.js --allowFailure)",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "eslint src --fix && eslint server --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --syntax scss",
    "prettier": "prettier --config .prettierrc \"./**/*.+(js|json|jsx)\" --write",
    "clean:example": "rnb --c",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "./**/*.+(js|json|jsx)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.1.3",
    "async-lock": "^1.2.4",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "confetti-js": "0.0.18",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "detect-browser": "^5.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "express-slow-down": "^1.3.1",
    "gg-components": "^20.1.0",
    "helmet": "^4.2.0",
    "hoist-non-react-statics": "3.3.2",
    "immer": "^8.0.0",
    "immutable": "^4.0.0-rc.12",
    "invariant": "2.2.4",
    "ip": "^1.1.5",
    "ip-parse": "^1.0.5",
    "konami": "^1.6.2",
    "lodash": "^4.17.20",
    "next": "^10.1.3",
    "next-images": "^1.6.2",
    "next-transpile-modules": "^4.1.0",
    "nodemailer": "^6.4.16",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-cookies": "^0.1.1",
    "react-dom": "^17.0.1",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.12.1",
    "react-redux": "7.0.2",
    "redis": "^3.0.2",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "redux-saga-routines": "^3.2.3",
    "reselect": "4.0.0",
    "safe-compare": "^1.1.4",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@r-next-b/cli": "^0.1.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "backstopjs": "^5.0.6",
    "coveralls": "^3.1.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-redux-saga": "^1.2.1",
    "husky": "^4.3.0",
    "jest-cli": "^26.6.3",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^1.3.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "prompt": "^1.1.0",
    "react-test-renderer": "^17.0.1",
    "redis-mock": "^0.54.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-skyscanner": "^3.0.3",
    "webpack": "^4.44.1",
    "whatwg-fetch": "^3.5.0"
  }
}