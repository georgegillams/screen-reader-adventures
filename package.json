{
  "name": "ScreenReaderAdventures",
  "version": "1.0.0",
  "description": "Learn to use a screen reader the fun way",
  "repository": {
    "type": "git",
    "url": "git://github.com/georgegillams/screen-reader-adventures.git"
  },
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "author": "George Gillams",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:clean": "rimraf ./build",
    "build:client": "cross-env NODE_PATH=./server/api:./app NODE_ENV=production webpack --config config/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:server": "cross-env NODE_ENV=development babel -d ./build/server ./server -s",
    "clean": "npm run test:clean && npm run build:clean",
    "dev": "cross-env redis-server & SECRET_API_KEY=asdfghjkl MONZO_ACCESS_PASSWORD=asdfghjkl npm run start:dev",
    "fix:adapter-core-5": "cp ./scripts/adapter_core5.js node_modules/webrtc-adapter/src/js/adapter_core5.js",
    "fix:package-lock": "sed -i '' -e 's/http:\\/\\//https:\\/\\//g' $(find . -name 'package-lock.json')",
    "link-helpers": "ln -Ffs $(pwd)/helpers $(pwd)/app/helpers && ln -Ffs $(pwd)/helpers $(pwd)/server/api/helpers",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "eslint . --ext .js,.jsx --fix",
    "lint:scss": "stylelint 'packages/**/*.scss' --syntax scss",
    "postinstall": "npm run fix:adapter-core-5 && npm run link-helpers",
    "prebuild": "npm run build:clean && npm run link-helpers",
    "pretest": "npm run test:clean",
    "prettier": "prettier --config .prettierrc --write \"**/*.js\"",
    "start": "npm run start:prod",
    "start:dev": "cross-env NODE_PATH=./server/api:./app NODE_ENV=development nodemon --ignore 'server/server_content' --ignore 'app/*' --exec babel-node server",
    "start:prod": "cross-env NODE_PATH=./build/server/api NODE_ENV=production node ./build/server",
    "start:test": "cross-env PORT=9001 NODE_PATH=./build/server/api NODE_ENV=production ALLOW_NON_SSL=true REDIS_MOCK=true node ./build/server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "backstopjs:test": "npm run start:test & (sleep 5 && node ./scripts/backstopjs/backstop.js)",
    "test": "cross-env NODE_ENV=test SECRET_API_KEY=asdfghjkl NODE_PATH=./server/api:./app jest --coverage",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test SECRET_API_KEY=asdfghjkl NODE_PATH=./server/api:./app jest --watchAll"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax scss"
    ]
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^3.0.6",
    "bibtex-parse-js": "0.0.24",
    "bpk-component-card": "^2.0.16",
    "bpk-component-checkbox": "^2.0.16",
    "bpk-component-code": "^2.0.16",
    "bpk-component-horizontal-nav": "^3.0.17",
