    "react-stripe-elements": "^6.1.2",
    "recompose": "^0.30.0",
    "redis": "^2.8.0",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-saga": "1.1.2",
    "reselect": "3.0.1",
    "safe-compare": "^1.1.4",
